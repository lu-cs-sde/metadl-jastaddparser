%terminals T3;
%terminals T4;
%terminals T1;
%terminals T2;
%typeof b = "B";
%typeof b_opt = "Opt";
%typeof c = "C";
%typeof a = "A";
%goal a;
a =
c.c {: return new A(new Opt(), c);:}
| b.b c.c {: return new A(new Opt(b), c);:}
| T3.T3 b_opt.b T4.T4 {: return new A(b);:}
| T3.T3 b.b {: return new A(b, new Opt());:}
| T3.T3 b.b c.c {: return new A(b, new Opt(c));:}
;
b =
T1.T1   ;
c =
T2.T2   ;
b_opt =
{: return new Opt(); :}
| b.n {: return new Opt(n); :}
;